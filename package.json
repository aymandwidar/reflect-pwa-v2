{
  "name": "reflect-pwa",
  "version": "3.0.0",
  "description": "Comprehensive mental wellness platform with AI CBT coach, multi-language support, guided programs, and wellness tools",
  "type": "module",
  "scripts": {
    "dev": "vite --port 3000",
    "dev:v2": "vite --config vite.config.v2.js",
    "dev:v3": "vite --config vite.config.v3.js",
    "build": "vite build",
    "build:v2": "vite build --config vite.config.v2.js",
    "build:v3": "vite build --config vite.config.v3.js && node -e \"require('fs').copyFileSync('dist-v3/index-v3.html', 'dist-v3/index.html')\"",
    "preview": "vite preview",
    "serve": "npx serve -s . -p 3000"
  },
  "dependencies": {
    "react": "^18.2.0",
    "react-dom": "^18.2.0"
  },
  "devDependencies": {
    "@vitejs/plugin-react": "^4.2.1",
    "vite": "^5.0.0"
  }
}
