{
  "version": 2,
  "name": "reflect-pwa-v4",
  "builds": [
    {
      "src": "package.json",
      "use": "@vercel/static-build",
      "config": {
        "distDir": "dist-v4"
      }
    }
  ],
  "routes": [
    {
      "src": "/manifest.json",
      "dest": "/dist-v4/assets/manifest.json"
    },
    {
      "src": "/sw.js",
      "dest": "/sw.js"
    },
    {
      "src": "/(.*)",
      "dest": "/dist-v4/$1"
    }
  ],
  "buildCommand": "npm run build:v4",
  "outputDirectory": "dist-v4",
  "framework": null,
  "installCommand": "npm install"
}
